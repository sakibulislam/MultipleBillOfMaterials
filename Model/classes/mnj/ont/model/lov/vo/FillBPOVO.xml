  Name="FillBPOVO"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnj.ont.model.lov.vo.FillBPOVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[SELECT ooha.cust_po_number bpo_no,
        ooha.attribute3 category,
        mmph.brand,
        mmph.fit,
        mmph.end_user,
        mmph.item_description,
        sum(oola.ordered_quantity) ordered_qty,  
       mmph.style_name,
       mmph.style_no,
       mmph.season,mmph.BUYER_ID Buyer_Id  ,
       ooha.HEADER_ID    
       FROM oe_order_headers_all ooha,
       mnj_mfg_precosting_h mmph,
       oe_order_lines_all   oola,
       ar_customers         ac
 where ooha.attribute5 = to_char(mmph.header_id)
   and ooha.header_id = oola.header_id
   and ac.CUSTOMER_ID = ooha.SOLD_TO_ORG_ID
  AND NOT EXISTS (SELECT 1 FROM  MNJ_BOM_BPO_LINES_ALL h
                  WHERE H.SALE_ORDER_ID=OOHA.HEADER_ID)
  /*and ooha.header_id not in
       (select h.sale_order_id
          from MNJ_BOM_BPO_LINES_ALL h
         where h.sale_order_id is not null)*/
 group by ooha.cust_po_number,
          ooha.attribute3,
          mmph.brand,
          mmph.fit,
          mmph.end_user,
          mmph.item_description,
          mmph.style_name,
          mmph.style_no,
          mmph.season,mmph.BUYER_ID,ooha.HEADER_ID]]>
  </SQLQuery>
